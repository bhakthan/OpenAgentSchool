<svg width="180" height="180" viewBox="0 0 180 180" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle with gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00A4EF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7FBA00;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <circle cx="90" cy="90" r="90" fill="url(#bgGradient)"/>
  
  <!-- Ladder representing progressive learning -->
  <!-- Left rail -->
  <rect x="50" y="30" width="12" height="120" rx="6" fill="#FFFFFF" opacity="0.9"/>
  
  <!-- Right rail -->
  <rect x="118" y="30" width="12" height="120" rx="6" fill="#FFFFFF" opacity="0.9"/>
  
  <!-- Progressive rungs with Microsoft brand colors -->
  <!-- Bottom rung - Red (Foundation) -->
  <rect x="50" y="130" width="80" height="12" rx="6" fill="#F25022"/>
  
  <!-- Third rung - Yellow (Development) -->
  <rect x="50" y="105" width="80" height="12" rx="6" fill="#FFB900"/>
  
  <!-- Second rung - Green (Growth) -->
  <rect x="50" y="80" width="80" height="12" rx="6" fill="#7FBA00"/>
  
  <!-- Top rung - Blue (Mastery) -->
  <rect x="50" y="55" width="80" height="12" rx="6" fill="#00A4EF"/>
  
  <!-- Sparkles for achievement -->
  <circle cx="40" cy="45" r="3" fill="#FFFFFF" opacity="0.8"/>
  <circle cx="140" cy="50" r="3" fill="#FFFFFF" opacity="0.8"/>
  <circle cx="35" cy="85" r="2" fill="#FFFFFF" opacity="0.6"/>
  <circle cx="145" cy="90" r="2" fill="#FFFFFF" opacity="0.6"/>
  <circle cx="40" cy="125" r="3" fill="#FFFFFF" opacity="0.8"/>
  <circle cx="140" cy="130" r="3" fill="#FFFFFF" opacity="0.8"/>
</svg>
